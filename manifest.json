{
  "schema": "synthia.addon.manifest.v1",
  "id": "demo",
  "name": "Demo Addon",
  "version": "0.1.0",
  "description": "A demo addon for Synthia.",
  "types": ["action","ui"],

  "author": "Synthia Team",
  "license": "MIT",
  "homepage": "https://github.com/YOUR_ORG/synthia-addon-demo",
  "docs": "https://github.com/YOUR_ORG/synthia-addon-demo/blob/main/README.md",
  "tags": ["demo", "example", "tutorial"],

  "core": { "min_version": "0.4.0", "max_version": "0.9.99" },
  "assets": { "icon": "frontend/img/icon.svg" },

  "deprecated": false,
  "replaced_by": null,

  "frontend": {
    "basePath": "/addons/demo",
    "hasSettingsPage": true,
    "showInSidebar": true,
    "sidebarLabel": "Demo",
    "showOnFrontpage": false
  },
  "backend": {
    "entry": "./backend/addon.py",
    "setup": "./backend/setup.py",
    "healthPath": "/status",
    "requiresConfig": []
  }
}
